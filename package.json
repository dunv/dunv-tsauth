{
    "name": "dunv-tsauth",
    "version": "0.0.24",
    "description": "Auth helpers",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "lint": "tsc --noEmit && eslint 'src/*.{js,ts,tsx}' --quiet --fix",
        "clean": "rimraf build",
        "build": "tsc --build",
        "start": "tsc -w",
        "test": "mocha -r ts-node/register tests/**/*.test.ts",
        "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
        "prepare": "npm run clean && npm run lint && npm run build",
        "createRelease": "npm version patch && git push --follow-tags"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dunv/dunv-tsauth.git"
    },
    "author": "Daniel Unverricht <daniel@unverricht.net>",
    "license": "MIT",
    "devDependencies": {
        "@types/chai": "^4.2.11",
        "@types/jwt-decode": "^2.2.1",
        "@types/mocha": "^7.0.2",
        "@types/react": "^16.9.25",
        "@typescript-eslint/eslint-plugin": "^2.25.0",
        "@typescript-eslint/parser": "^2.25.0",
        "chai": "^4.2.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "mocha": "^7.1.1",
        "nyc": "^15.0.0",
        "prettier": "^2.0.2",
        "react": "^16.13.1",
        "rimraf": "^3.0.2",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.7.0",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "autobind-decorator": "^2.4.0",
        "axios": "^0.21.1",
        "es-cookie": "^1.3.2",
        "jwt-decode": "^2.2.0"
    }
}
